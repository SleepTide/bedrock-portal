{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2022",
        "moduleResolution": "node",
        "rootDir": "src",
        "outDir": "dist",
        "removeComments": true,
        "noImplicitAny": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "esModuleInterop": true
    },
    "exlude": [
      "node_modules"
    ]
}
